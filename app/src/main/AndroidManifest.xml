<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ruanx.cashbook">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddItemActivity"
            android:windowSoftInputMode="adjustPan" />
        <!-- <activity android:name=".ItemInfoActivity" /> -->
        <activity android:name=".BackGroundActivity" />
        <activity android:name=".AccountActivity" />
        <!-- <activity android:name=".LabelActivity" /> -->
        <activity android:name=".selectLabelActivity" />
        <activity android:name=".ViewBrActivity" />
        <activity android:name=".AddBRActivity" />
        <activity android:name=".Pie_Activity" />
        <activity android:name=".Password_Actiivity" />
        <activity
            android:name=".SearchActivity"
            android:noHistory="true"
            android:windowSoftInputMode="adjustPan" />
        <activity android:name=".OutputActivity"></activity>
        <activity android:name=".ExchangeActivity"/>
    </application>

</manifest>